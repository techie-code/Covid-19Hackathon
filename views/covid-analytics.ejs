    <div id="page-content-wrapper">
        <nav class="navbar navtit navbar-expand-lg py-4 px-4">
            <div class="d-flex align-items-center">
                <i class="fas fa-align-left primary-text fs-4 me-3" id="menu-toggle"></i>
                <h2 class="titlenav">COVID-19 Analytics</h2>
            </div>
        </nav>
    <div class="analytic">


    <div class="contentss">
        <div class="container contbox">
                    <div class="row">

                        <div class="col-sm-12 col-md-6 col-lg-3">
                            <div class="box1">
                                <div class="country-select" id="country-select">
                                    <div class="country-select-toggle" id="country-select-toggle">
                                        <span>
                                            All Countries
                                        </span>
                                        <i class="bx bx-chevron-down"></i>
                                    </div>
                                    <div class="country-select-list" id="country-select-list">
                                        <input type="text" placeholder="Search country name">
                                    </div>
                                </div>
                            </div>
                        </div>

                        <div class="col-sm-12 col-md-6 col-lg-3">
                            <div class="box1 box-hover">
                                <div class="count count-confirmed">
                                    <div class="count-icon">
                                        <i class="fas fa-clipboard-check"></i>
                                    </div>
                                    <div class="count-info">
                                        <h5 id="confirmed-total">0</h5>
                                        <span>confirmed</span>
                                    </div>
                                </div>
                            </div>
                        </div>

                        <div class="col-sm-12 col-md-6 col-lg-3">
                            <div class="box1 box-hover">
                                <div class="count count-recovered">
                                    <div class="count-icon">
                                        <i class="fas fa-virus-slash"></i>
                                    </div>
                                    <div class="count-info">
                                        <h5 id="recovered-total">0</h5>
                                        <span>recovered</span>
                                    </div>
                                </div>
                            </div>
                        </div>


                        <div class="col-sm-12 col-md-6 col-lg-3">
                            <div class="box1 box-hover">
                                <div class="count count-death">
                                    <div class="count-icon">
                                        <i class="fas fa-skull-crossbones"></i>
                                    </div>
                                    <div class="count-info">
                                        <h5 id="death-total">0</h5>
                                        <span>deaths</span>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>

                        <div class="row">

                        <div class="col-sm-12 col-md-12 col-lg-6">
                            <div class="box1">
                                <div class="box-header">
                                    COVID-19 CASES 
                                </div>
                                <div class="box-body">
                                    <div id="all-time-chart"></div>
                                </div>
                            </div>
                        </div>
                        <div class="col-sm-12 col-md-12 col-lg-6">
                        <div class="box1">
                            <div class="box-header">
                                last 30 days
                            </div>
                            <div class="box-body">
                                <div id="days-chart"></div>
                            </div>
                        </div>
 
                    </div>
                </div>
                </div>



                <div class="row">

                     <div class="col-sm-12 col-md-12 col-lg-6">

                    <div class="box1">
                        <div class="box-header">
                            recovery Rate
                        </div>
                        <div class="box-body">
                            <div id="recover-rate-chart"></div>
                        </div>
                    </div>
                </div>


                    <div class="col-sm-12 col-md-12 col-lg-6">
                    <div class="box1">
                        <div class="box-header">
                            Death Rate
                        </div>
                        <div class="box-body">
                            <div id="death-rate-chart"></div>
                        </div>
                    </div>
                </div>
                <div class="col-sm-12 col-md-12 col-lg-12">
                    <div class="table-responsive">
                        <h1>Top countries: </h1>
                <table class="table table-countries" id="table-countries">
                    <thead>
                        <tr>
                            <th class="align"scope="col">
                                Country
                            </th>
                            <th class="align" scope="col">Confirmed</th>
                            <th class="align" scope="col">Recovered</th>
                            <th class="align" scope="col">Deaths</th>
                        </tr>
                    </thead>
                    <tbody>
                        <tr>
                            <th scope="row"></th>
                            <td>Searching...</td>
                            <td>Searching...</td>
                            <td>Searching...</td>
                            <td>Searching...</td>
                        </tr>
                    </tbody>
                </table>
                </div>
            </div>
            </div>

        </div>
    </div>

    <div class="loader">
        <i class="bx bxs-virus bx-spin"></i>
    </div>
</div>
</div>
